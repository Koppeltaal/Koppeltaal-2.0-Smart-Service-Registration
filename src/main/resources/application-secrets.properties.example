# Copy this file to application-secrets.properties and fill in your actual values
# application-secrets.properties is gitignored and will not be committed

# Database credentials
spring.datasource.username=<your-db-username>
spring.datasource.password=<your-db-password>

# Authorization tokens
authorizations.endpoint.token=<your-auth-token>

# OIDC credentials
oidc.clientId=<your-oidc-client-id>
oidc.clientSecret=<your-oidc-client-secret>

# FHIR Smart Service
fhir.smart.service.clientId=<your-fhir-client-id>

# JWKS signing keys
jwks.signingPublicKey=<your-jwks-public-key>
jwks.signingPrivateKey=<your-jwks-private-key>
